{
  "manifest_version": 2,

  "name": AI2BlockRescaler",
  "description": "Rescales the block editing canvas of the App Inventor 2 by MIT to zoom in or out",
  "version": "0.0.1",

  "page_action": {
    "default_icon": {
            "48": "images/icon48.png" 
    },
    "default_title": "AI2 Block Resizer",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "activeTab"
  ]
}
